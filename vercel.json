{
  "version": 2,
  "builds": [{ "src": "main.js", "use": "@vercel/node" }],
  "routes": [
    { "src": "/socket.io/(.*)", "dest": "/main.js" },
    { "src": "/(.*)", "dest": "/frontend/index.html" }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "functions": { "api/*.js": { "runtime": "nodejs14.x" } }
}
